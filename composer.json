{
    "name": "mikehaertl/phpwkhtmltopdf",
    "description": "A slim PHP wrapper around wkhtmltopdf with an easy to use and clean OOP interface",
    "keywords": ["pdf"],
    "homepage": "http://mikehaertl.github.com/phpwkhtmltopdf/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Haertl",
            "email": "haertl.mike@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.0.0",
        "mikehaertl/php-tmpfile": "*",
        "mikehaertl/php-shellcommand": "~1.0.2"
    },
    "autoload": {
        "psr-4": {
            "mikehaertl\\wkhtmlto\\": "src/"
        }
    },
    "suggest": {
        "profburial/wkhtmltopdf-binaries-osx": "wkhtmltopdf static linked osx binaries.",
        "profburial/wkhtmltopdf-binaries-centos6": "wkhtmltopdf static linked centos6 binaries.",
        "profburial/wkhtmltopdf-binaries-precise": "wkhtmltopdf static linked precise binaries.",
        "profburial/wkhtmltopdf-binaries-trusty": "wkhtmltopdf static linked trusty binaries.",
        "profburial/wkhtmltopdf-binaries-wheezy": "wkhtmltopdf static linked wheezy binaries."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
